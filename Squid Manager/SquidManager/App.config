<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHTTPBinding" />
            </basicHttpBinding>
        </bindings>
        <services>
            <service name="sqmgr_wcf_svc.SqMgrSvc">
                <clear />
                <endpoint binding="basicHttpBinding" bindingConfiguration="BasicHTTPBinding"
                  name="BasicHTTP" contract="sqmgr_wcf_svc.ISqMgr" listenUriMode="Explicit" />
                <endpoint address="mex" binding="mexHttpBinding" name="Mex" contract="IMetadataExchange"
                  listenUriMode="Explicit" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://127.0.0.1:5656/squid_manager" />
                    </baseAddresses>
                </host>
            </service>
        </services>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true"
                      httpGetBinding="webHttpBinding" httpGetBindingConfiguration=""/>
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
    </system.serviceModel>
</configuration>